"use strict";(function(){function a(){return!navigator.brave&&!navigator.userAgent.match(/Edg|OPR/)&&navigator.userAgent.match(/Chrome|CriOS/)}function b(){return navigator.brave&&navigator.brave.isBrave}a()&&document.querySelector("html").classList.add("is-chrome"),b()&&document.querySelector("html").classList.add("is-brave"),is_iOS()&&document.querySelector("html").classList.add("is-ios"),is_Android()&&document.querySelector("html").classList.add("is-android")})();function is_iOS(){return(/iPad|iPhone|iPod/.test(navigator.platform)||navigator.platform==="MacIntel"&&navigator.maxTouchPoints>1)&&!window.MSStream}function is_Android(){return/Android/.test(navigator.userAgent)}