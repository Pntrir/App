"use strict";function initLocalization(b){var c=e("lang")||h(),a=g(c),d=i();!/en/.test(c)&&a&&a!==location.pathname&&(location.href=d?"".concat(a,"?ref=").concat(d):a),window.addEventListener("DOMContentLoaded",f);function f(){var a=document.querySelector("#language-switcher");a.addEventListener("change",function(){e("lang",this.value,{path:"/"}),location.href=b[this.value]})}function g(a){return b[a]||b[a.slice(0,2)]}function h(){return(navigator.languages&&navigator.languages[0]||navigator.language||navigator.userLanguage).toLowerCase()}function i(){var b=/(?:\?|&)ref=([A-Za-z]{3}-?[0-9]{3})/i,c,a;return window.URLSearchParams?(c=new URLSearchParams(location.search),a=c.get("ref"),a&&/[A-Za-z]{3}-?[0-9]{3}/i.test(a)?a:null):b.test(location.search)?location.search.match(b)[1]:null}function e(h,a,b){var f,g,d,c,e;if(navigator.cookieEnabled)if(typeof a=="undefined"){{d=document.cookie.split(";");for(c=0;c<d.length;c++)if(f=d[c].substr(0,d[c].indexOf("=")).trim(),g=d[c].substr(d[c].indexOf("=")+1),f===h)return decodeURIComponent(g)}}else b=b||{},a?a=encodeURIComponent(a):(a="",b.expires=-365),b.expires&&(e=new Date,e.setDate(e.getDate()+b.expires),a+="; expires="+e.toUTCString()),b.domain&&(a+="; domain="+b.domain),b.path&&(a+="; path="+b.path),a+="; secure=true",document.cookie=h+"="+a}}